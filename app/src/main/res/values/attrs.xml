<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BoxedEditText">
        <attr name="hint" format="string" />
        <attr name="prefix" format="string" />
        <attr name="inputType" format="integer">
            <enum name="text" value="1" />
            <enum name="email" value="2" />
            <enum name="phone" value="3" />
            <enum name="number" value="4" />
            <enum name="uri" value="5" />
        </attr>
    </declare-styleable>
</resources>